{
  "id": "{{linker-cmiserver.name}}",
  "instances": {{linker-cmiserver.instances}},
  "cpus": {{linker-cmiserver.cpus}},
  "mem": {{linker-cmiserver.mem}},
  "cmd": "{{linker-cmiserver.command}}",
  "maintainer": "support@linkernetwork.com",
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "{{resource.assets.container.docker.linker-cmiserver-docker}}",
       "portMappings": [
        {
          "containerPort": {{linker-cmiserver.serviceport}},
          "hostPort": 0,
          "servicePort": {{linker-cmiserver.serviceport}},
          "protocol": "tcp",
          "labels": {}
        }
      ],
      "network": "BRIDGE",
      "privileged": true
    }
  },
  "labels": {
    "HAPROXY_GROUP": "{{linker-cmiserver.haproxy-group}}",
    "DCOS_PACKAGE_IS_FRAMEWORK": "false"
  },
  "env": {
   {{#linker-cmiserver.cassandraips}}
    "CASSANDRA_IPS": "{{linker-cmiserver.cassandraips}}"
   {{/linker-cmiserver.cassandraips}}
  },
  "uris": [
     {{#linker-cmiserver.template-url}} 
      "{{linker-cmiserver.template-url}}"
     {{/linker-cmiserver.template-url}}
   ]
   
}
